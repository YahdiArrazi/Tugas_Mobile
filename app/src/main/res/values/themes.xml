<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PrioTodoApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/yellowgreen</item>
        <item name="colorPrimaryVariant">@color/green</item>
        <item name="colorOnPrimary">@color/greencyan</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/mediumcyan</item>
        <item name="colorSecondaryVariant">@color/darkcyan</item>
        <item name="colorOnSecondary">@color/cyanblue</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>